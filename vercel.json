{
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*\\.php)$",
      "dest": "/$1"
    },
    {
      "src": "/(css|js|images|uploads)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/",
      "dest": "/index.php"
    }
  ]
}
